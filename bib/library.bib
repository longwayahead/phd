@book{GrandjeanOrgelundOper2015,
  title = {Orgel und Oper. Georges Schmitt 1821--1900: Ein deutsch-franz\"osischer Musiker in Paris. Biographie und Werk mit einem Werkverzeichnis},
  shorttitle = {Orgel und Oper},
  author = {Grandjean, Wolfgang},
  date = {2015},
  publisher = {{Georg Olms Verlag}},
  location = {{Hildesheim}},
  abstract = {Georg bzw. Georges Schmitt (1821-1900) ist ein \guilsinglright Grenzg\"anger\guilsinglleft{} zwischen deutscher und franz\"osischer Musik. Als Organist und Reformer der Kirchenmusik l\"asst er sich unter dem Aspekt des \guillemotright Kulturtransfers\guillemotleft{} als Vermittler im deutsch-franz\"osischen Kulturfeld betrachten. In Trier geboren und als musikalische Fr\"uhbegabung mit 14 Jahren bereits Domorganist in Trier, wanderte er 1844 nach Paris aus, wo er mehr als ein halbes Jahrhundert lang die Entwicklung der franz\"osischen Musik in einer ihrer dynamischsten Phasen erlebte.Als Organist an der grandiosen Orgel von Saint-Sulpice gewann er eine prestigetr\"achtige Trib\"une f\"ur sein Wirken in der Kirchenmusikreform. Er ver\"offentlichte das Roret-Handbuch L'Organiste praticien (1855) sowie die Anthologie Le Mus\'ee de l'organiste (1857/1858) und verfasste zahlreiche Zeitschriftenbeitr\"age. \textendash{} Als Opernkomponist vermochte er jedoch nicht, trotz einiger Auff\"uhrungen an Pariser Theatern, sich durchzusetzen, \"ahnlich wenig mit seinen Chorsymphonien und Kantaten. (Seine \guillemotright Symphonie dramatique\guillemotleft{} Le Sina\"i wird nach 135 Jahren im September 2014 in Trier wieder aufgef\"uhrt.)Die politischen und kulturellen Entwicklungen in Frankreich bilden sich in H\"ohen und Tiefen seiner k\"unstlerischen Laufbahn ab. Nach 1871 wurde er zum Verlierer der R\"uckbesinnung der franz\"osischen Musik auf ihr nationales Erbe, und als \guillemotright franz\"osischer Komponist\guillemotleft{} konnte er sich in Zeiten der \guillemotright Erbfeindschaft\guillemotleft{} auch in Deutschland nicht mehr durchsetzen, wo man ihn in Trier auf den \guillemotright S\"anger des Moselliedes\guillemotleft{} reduzierte.Das Leben von Georges Schmitt ist paradigmatisch f\"ur die Ebene der Musikkultur, die als \guilsinglright Basis\guilsinglleft{} zunehmend das Interesse der Historiographie findet.Georg or Georges Schmitt (1821-1900) crosses boundaries between German and French music. As an organist and reformer of church music he can be seen from the perspective of `cultural transfer' as a mediator in the sphere of Franco-German cultural relations. Born in Trier, he was a musical prodigy who became organist of Trier Cathrdral at the age of 14. In 1844 he emigrated to Paris where for over half a century he experienced one of the most dynamic phases in the development of French music.As organist at the grand instrument in Saint-Sulpice he had a prestigious platform for his activity in reforming church music. He published the Roret Handbook L'organiste practicien (1855) and the anthology Le muse\'e de l'organiste (1857-58), and wrote numerous journal articles. However, despite some performances in Parisian theatres, he was unable to win recognition as an opera composer, and had similarly little success with his choral symphonies and cantatas. (His `Symphonie dramatique' Le Sina\"i was performed again after 135 years in Trier in 2014.)Political and cultural developments in France are reflected in the highs and lows of his artistic career. After 1871 he lost out as French music looked back to its national heritage, but as a `French composer' he could not find recvognition in Germany either in the era of the `ancient emnity' , and in Trier was seen merely as `the singer of the Mosellied'. Georges Schmitt's life is paradignamtic of the levels of musical culture which are increasingly attracting the interest of historians as a `basis.'},
  isbn = {978-3-487-15181-6},
  langid = {ngerman},
  pagetotal = {544},
  keywords = {Music / History & Criticism}
}

